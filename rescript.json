{
  "name": "vite-react-rescript-starter",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "uncurried": true,
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@greenlabs/rescript-react-hook-form"
  ],
  "bs-dev-dependencies": [],
  "bsc-flags": ["-open RescriptCore", "-open Shadcn"]
}
